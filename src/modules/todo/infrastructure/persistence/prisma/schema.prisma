generator kysely {
  provider     = "prisma-kysely"
  output       = "../kysely/"
  fileName     = "todo.models.kysely.d.ts"
  enumFileName = "todo.enums.kysely.d.ts"
  camelCase    = true
}

datasource db {
  provider = "postgresql"
}

model migrations {
  id        Int    @id @default(autoincrement())
  timestamp BigInt
  name      String @db.VarChar
}

model todo {
  id         String   @id
  title      String
  is_done    Boolean  @default(false)
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}
